[general]
format=wav49
maxmsg=200
maxsecs=240	; maximo de 3 min para cada mensagem
minsecs=3	; descartar mensagens com menos que essa duracao
maxgreet=60	; duracao da mensagem de recepcao
skipms=3000	; * e # devem avancar ou voltar 3s
maxsilence=5	; tempo maximo de silencio antes de encerrar a gravacao
maxlogins=5	; maximo de vezes em que pode errar a senha
moveheard=yes	; move mensagens ouvidas para a pasta Old
saycid=yes
review=yes
messagewrap=yes
language=br

[zonemessages]

[default]
4001 => 12345,Ramal 4001,,,envelope=yes|review=yes
4002 => 12345,Ramal 4002
4003 => 12345,Ramal 4003
4004 => 12345,Ramal 4004
